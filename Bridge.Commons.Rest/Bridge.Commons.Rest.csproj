<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <PackageId>Bridge.Commons.Rest</PackageId>
        <Title>Bridge.Commons.Rest</Title>
        <Version>1.0.0</Version>
        <Authors>Robson Leao</Authors>
        <Company>Bridge Core</Company>
        <Description>
            Rest package: Get, Post, Put, Delete, Patch, etc.
